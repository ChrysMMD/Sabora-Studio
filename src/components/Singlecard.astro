---
const {data} = Astro.props;
---
<a href={`artwork/${data.slug}`}>
    <article>
        <div>
            <h2>{data.title}</h2>
            <p>{data.description}</p>
            <a href={`artwork/${data.slug}`}></a>
        </div>
    </article>
</a>

<style>
     img{
        border-radius: 25px;  
     }
</style>